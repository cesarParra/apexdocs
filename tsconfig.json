{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "lib": [
      "ES2021"
    ],
    "baseUrl": "./",
    "paths": {
      "#utils/*": ["src/util/*"],
    }
  },
  "include": [
    "src", "jest.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/__tests__/*"
  ]
}
