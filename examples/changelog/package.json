{
  "name": "changelog-example",
  "scripts": {
    "docs:clean": "rimraf docs",
    "docs:build": "ts-node ../../src/cli/generate.ts changelog",
    "docs:help": "ts-node ../../src/cli/generate.ts changelog --help",
    "docs:gen": "npm run docs:clean && npm run docs:build"
  },
  "devDependencies": {
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "rimraf": "^5.0.7"
  },
  "apexdocs": {
    "previousVersionDir": "previous",
    "currentVersionDir": "current",
    "scope": ["global", "public", "private"]
  }
}
